#include "intpult.h"
//============================================================================================================
//+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
//============================================================================================================
void setup() {
  Serial.begin(115200);
  setup_CHANEL();
}
//============================================================================================================
//+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
//============================================================================================================

void loop()
{
  long mic;
  mic = micros();
  ScanCHANEL();
  mic = micros() - mic;
  Serial.print(mic); Serial.print(" = ");
  Serial.print(Canal1Time); Serial.print(" = ");
  Serial.print(Canal2Time); Serial.print(" = ");
  Serial.print(Canal3Time); Serial.print(" = ");
  Serial.print(Canal4Time); Serial.print(" = ");
  Serial.println("==========================================");
  delay(200);
}
